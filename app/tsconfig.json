{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "outDir": "../.cache/typescript-app",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "@/*": ["./*"]
    },
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.json"],
  "exclude": ["dist/**/*", "vite.config.ts", "core/config.ts"],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
